<svg width="800" height="200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="200" fill="#f5f5f5" stroke="#e0e0e0" stroke-width="1"/>
  
  <!-- Container for buttons -->
  <g transform="translate(50, 50)">
    <!-- Button 1: Shrnutí -->
    <g transform="translate(0, 0)">
      <rect x="0" y="0" width="100" height="100" rx="8" fill="white" stroke="#e0e0e0" stroke-width="1"/>
      <g transform="translate(50, 30)">
        <path d="M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <polyline points="14 2 14 8 20 8" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <line x1="16" y1="13" x2="8" y2="13" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <line x1="16" y1="17" x2="8" y2="17" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
      </g>
      <text x="50" y="85" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Shrnutí</text>
    </g>
    
    <!-- Button 2: Studijní text -->
    <g transform="translate(120, 0)">
      <rect x="0" y="0" width="100" height="100" rx="8" fill="white" stroke="#e0e0e0" stroke-width="1"/>
      <g transform="translate(50, 30)">
        <path d="M4 19.5A2.5 2.5 0 0 1 6.5 17H20" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <path d="M6.5 2H20v20H6.5A2.5 2.5 0 0 1 4 19.5v-15A2.5 2.5 0 0 1 6.5 2z" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
      </g>
      <text x="50" y="85" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Studijní text</text>
    </g>
    
    <!-- Button 3: Audio -->
    <g transform="translate(240, 0)">
      <rect x="0" y="0" width="100" height="100" rx="8" fill="white" stroke="#e0e0e0" stroke-width="1"/>
      <g transform="translate(50, 30)">
        <polygon points="11 5 6 9 2 9 2 15 6 15 11 19 11 5" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-6.5, -12)"/>
        <path d="M15.54 8.46a5 5 0 0 1 0 7.07" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-6.5, -12)"/>
        <path d="M19.07 4.93a10 10 0 0 1 0 14.14" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-6.5, -12)"/>
      </g>
      <text x="50" y="85" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Audio</text>
    </g>
    
    <!-- Button 4: Další zdroje -->
    <g transform="translate(360, 0)">
      <rect x="0" y="0" width="100" height="100" rx="8" fill="white" stroke="#e0e0e0" stroke-width="1"/>
      <g transform="translate(50, 30)">
        <path d="M18 13v6a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h6" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <polyline points="15 3 21 3 21 9" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <line x1="10" y1="14" x2="21" y2="3" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
      </g>
      <text x="50" y="85" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Další zdroje</text>
    </g>
    
    <!-- Button 5: Kartičky -->
    <g transform="translate(480, 0)">
      <rect x="0" y="0" width="100" height="100" rx="8" fill="white" stroke="#e0e0e0" stroke-width="1"/>
      <g transform="translate(50, 30)">
        <rect x="1" y="4" width="22" height="16" rx="2" ry="2" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <line x1="1" y1="10" x2="23" y2="10" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
      </g>
      <text x="50" y="85" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Kartičky</text>
    </g>
    
    <!-- Button 6: Kvíz -->
    <g transform="translate(600, 0)">
      <rect x="0" y="0" width="100" height="100" rx="8" fill="white" stroke="#e0e0e0" stroke-width="1"/>
      <g transform="translate(50, 30)">
        <circle cx="12" cy="12" r="10" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <path d="M9.09 9a3 3 0 0 1 5.83 1c0 2-3 3-3 3" fill="none" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
        <line x1="12" y1="17" x2="12.01" y2="17" stroke="#333" stroke-width="2" transform="scale(0.8) translate(-12, -12)"/>
      </g>
      <text x="50" y="85" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Kvíz</text>
    </g>
  </g>
</svg>

